<script>
    var Species = "2.2", BtnId;
    $('#parent1').click(function () {
        BtnId = "parent1";
        uploadimgs();
    });
    $('#parent2').click(function () {
        BtnId = "parent2";
         uploadimgs();
      //  SeaveLocal('A4BG2I_jAk71lfPzk0gxfQApgoeJNN0mMo0qLS7jcCjgldJ23kupHAnjljzl0Rs3');
    });


    //种类（1.1：娃像谁 3人照片 1张；1.2：娃像谁 单人照 3张 ；2.1：夫妻相 双人照 1张；2.2夫妻相 2张）
    function SeaveLocal(serverId) {
        $.get('downtemp', { serverId: serverId }, function (data) {
            if (data.isLock == 1) {
                $('#' + BtnId + '').html("<img class=\"weui_media_appmsg_thumb\" src=\"../Images/" + data.ImgUrl + "\" alt=\"\">")

                BtnId = "";
            } else {
                alert("上传失败");
            }

        });
    }
    $('#uploadImage').click(function () {
        //  $('.weui_media_appmsg_thumb')[0].src
        if ($('.weui_media_appmsg_thumb').length==2) {
            $.get('Compare', { Species: Species, Dad: $('.weui_media_appmsg_thumb')[0].src, Mom: $('.weui_media_appmsg_thumb')[1].src }, function (data) {
                if (data.isLock == 1) {
                    alert("测试成功");
                } else {
                    alert(data.ImgMsg);
                }

            });
        } else {
            alert('图片上传不完整，无法对比！');
        }

    });
</script>
